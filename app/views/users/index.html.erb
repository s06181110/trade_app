
<div class="layout">
	<h1 class="page-title">ユーザー一覧</h1>
	<div class="table-main">
		<table border="1">
		<thead>
			<tr>
				<th>名前</th>
				<th>メアド</th>
			</tr>
		</thead>
		<tbody>
			<% @users.each do |user| %>
				<tr>
					<td><%= user.name %></td>
					<td><%= user.email %></td>
				</tr>
			<% end %>
		</tbody>
		</table>
	</div>
</div>
 